{
  "name": "fashion-ecommerce-shop",
  "version": "1.0.0",
  "description": "Fashion ecommerce with Fastify and TypeScript",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node src/scripts/dev-migrate.ts && ts-node-dev --respawn --clear --transpile-only -r tsconfig-paths/register src/server.ts",
    "studio": "npx prisma studio",
    "migrate:deploy": "npx prisma studio"
  },
  "_moduleAliases": {
    "@": "dist",
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@modules": "dist/controllers",
    "@middleware": "dist/modules",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@types": "dist/types",
    "@utils": "dist/utils"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/adapter-neon": "^6.12.0",
    "@prisma/client": "^6.10.1",
    "bcrypt": "^6.0.0",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "fastify-zod": "^1.4.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "otplib": "^12.0.1",
    "prisma": "^6.10.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "zod": "^3.25.60",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.29",
    "@types/ws": "^8.18.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
